<table id="inventory" class="display" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th>Image</th>
            <th>Name</th>
            <th>Description</th>
            <th>SKU</th>
            <th>Barcode</th>
            <th>Category</th>
            <th>Quantity</th>
            <th>Price</th>
            <th>Supplier</th>
        </tr>
    </thead>
    <tbody>
        {{#each inventory}}
            <tr>
                <td><img src="{{this.productImage}}" width="50px" height="50px" alt="{{this.productName}} {{this.productDescription}}"></td>
                <td data-search="{{this.productName}}">{{this.productName}}</td>
                <td>{{this.productDescription}}</td>
                <td>{{this.sku}}</td>
                <td>{{this.barcode}}</td>
                <td data-search="{{this.category}}">{{this.category}}</td>
                <td>{{this.quantity}}</td>
                <td>${{this.price}}</td>
                <td>{{this.supplier}}</td>
            </tr>
        {{/each}} 
    </tbody>
</table>